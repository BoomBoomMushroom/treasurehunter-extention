{
  "manifest_version": 2,
  "name": "HighSeas Shop Favourite Button",
  "version": "1.1",
  "description": "Adds a favourite button to the HighSeas Shop items.",
  "permissions": ["storage", "activeTab"],
  "browser_action": {
    "default_popup": "popup.html"
  },
  "content_scripts": [
    {
      "matches": ["https://highseas.hackclub.com/*"],
      "js": ["content.js"]
    }
  ]
}
